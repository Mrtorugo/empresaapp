package licaolaercio;


import javax.swing.*;

public class jopitionpane {
	public static void main(String args[]) {
		double n1, n2, resp = 0;
		String jo = "";
		
		jo = JOptionPane.showInputDialog(null, "Insira o primeiro numero 1");
		n1 = Double.parseDouble(jo);
		
		jo = JOptionPane.showInputDialog(null, "Insira o segundo numero 2");
		
		n2 = Double.parseDouble(jo);
		
		resp = n1*n2;
		
		JOptionPane.showMessageDialog(null, "A multiplicação ?"+resp);
	}
}
 

